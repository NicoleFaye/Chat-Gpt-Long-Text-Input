{

  "description": "A firefox Browser plugin for Chat Gpt that automates the entry of a large peice of text a little bit at a time.",
  "manifest_version": 2,
  "name": "Chat Gpt Long Text Input",
  "version": "1.1.1",
  "homepage_url": "https://github.com/NicoleFaye/Chat-Gpt-Long-Text-Input",
  "icons": {
    "48": "icons/Red48.png"
  },

  "permissions": [
    "activeTab"
  ],


  "browser_action": {
    "default_icon": "icons/Red32.png",
    "default_title": "Chat Gpt Long Text Input",
    "default_popup": "popup/ChatGptLongTextInput.html"

  }
}
